freebcp.1: freebcp.txt
	$(TXT2MAN) $(MANOPTS) $(srcdir)/freebcp.txt >.$@
	if test ! -s .$@; then echo 'unable to generate $@'; fi
	test -s .$@
	mv .$@ $@

